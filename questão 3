function separarParesImpares(arr) {
  
  const resultado = {
    pares: [],
    impares: []
  };

  for (let i = 0; i < arr.length; i++) {
    if (arr[i] % 2 === 0) {
      resultado.pares.push(arr[i]);  
    } else {
      resultado.impares.push(arr[i]);  
    }
  }

  return resultado;
}

const numeros = [1, 2, 3, 4, 5, 6, 7, 8, 9];
const resultado = separarParesImpares(numeros);
console.log(resultado);
